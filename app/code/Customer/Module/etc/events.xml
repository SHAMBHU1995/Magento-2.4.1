<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="customer_login">
        <observer name="customer_login_observer" instance="Customer\Module\Observer\CustomerLogin" />
    </event>

    <event name="customer_logout">
        <observer name="customer_logout_observer" instance="Customer\Module\Observer\CustomerLogout" />
    </event>

    <event name="backend_auth_user_login_success">
        <observer name="backend_auth_user_login_success" instance="Customer\Module\Observer\AdminLogin" />
    </event>
</config>